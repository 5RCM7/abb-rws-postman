				{
					"name": "Change-count",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extraire la valeur du <lvalue> dans la réponse XHTML\r",
									"let match = pm.response.text().match(/<span class=\"lvalue\">(\\d+)<\\/span>/);\r",
									"\r",
									"if (match) {\r",
									"    let ccountValue = match[1];\r",
									"    // Met à jour la variable au niveau de la collection\r",
									"    pm.collectionVariables.set(\"ccount\", ccountValue);\r",
									"    console.log(\"✅ ccount mis à jour :\", ccountValue);\r",
									"} else {\r",
									"    console.log(\"⚠️ Impossible d’extraire la valeur du change-count.\");\r",
									"}\r",
									""
								],
